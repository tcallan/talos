<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>Talos</Title>
    <Description>Generate and apply diffs between JSON documents</Description>
    <Authors>tcallan</Authors>
    <Copyright>Copyright Â© 2018 tcallan</Copyright>
    <VersionPrefix>1.0.0</VersionPrefix>
    <VersionSuffix>beta3</VersionSuffix>
    <PackageLicenseUrl>https://github.com/tcallan/talos/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/tcallan/talos</PackageProjectUrl>
    <RepositoryUrl>https://github.com/tcallan/talos</RepositoryUrl>
    <PackageTags>json;fsharp;f#;functional</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="ColExt.fs" />
    <Compile Include="Distance.fs" />
    <Compile Include="Pointer.fs" />
    <Compile Include="Patch.fs" />
    <Compile Include="Library.fs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Chiron" Version="7.0.0-beta-180105" />
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
  </ItemGroup>
</Project>